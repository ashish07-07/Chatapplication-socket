

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model message{
   id Int @id @default(autoincrement())
   from String
   to String
   message String
}

model User{

id Int @id @default(autoincrement())
email String @unique
name String
phonenumber String @unique
password String

}



